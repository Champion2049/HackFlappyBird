class Splash {
  // show splash
  function void show() {
    do ImageSetUp.blocksSetUp();
    do drawFlappyWord.drawFlappyWord(120, 70);
    do drawBirdWord.drawBirdWord(260, 70);
    do ImageSetUp.drawBirdEffect();
    do Sys.wait(200);
    do WelcomePage.writeInstructions();
    return;
  }

  method void dispose() {
    do Memory.deAlloc(this);
    return;
  }
}
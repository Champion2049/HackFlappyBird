class MyMath
{
  /*
  Modulus operator function.
  */
  function int mod(int dividend, int divisor)
  {
    var int quotient;

    let quotient = dividend / divisor;

    return (dividend - (quotient * divisor));
  }

  /*
  Absolute value function.
  */
  function int abs(int x) {
        if (x < 0) {
            return -x;
        }
        return x;
    }
}
